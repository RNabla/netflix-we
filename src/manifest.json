{
    "description": "Utility extension for Netflix",
    "homepage_url": "https://github.com/RNabla/netflix-we",
    "manifest_version": 2,
    "name": "Netlix Utility",
    "version": "1.0.0",
    "permissions": [
        "storage",
        "notifications"
    ],
    "options_ui": {
        "page": "options.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.netflix.com/*"
            ],
            "js": [
                "playback.js"
            ]
        }
    ]
}